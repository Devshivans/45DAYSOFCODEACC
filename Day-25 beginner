#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int a,b,c,count=0;
	    cin>>a>>b>>c;
	    for(int i=0;i<=a;i++)
        {
            if(b*i==c)
            {
                count++;
                cout << "yes" << endl;
            }
        }
        if(count==0)
        cout << "no" << endl;
	}
	return 0;
}
