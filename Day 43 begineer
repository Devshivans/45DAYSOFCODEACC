#include <iostream>
using namespace std;
void prime(int n){
	    for(int i=2;i<n/2;i++){
         if(n%i==0){
             cout<<"NO its not prime"<<endl;
             return;
         }
        
     }
     cout<<"yes its prime"<<endl;

}
int main() {
	// your code goes here
 int T;
 cout<<"enter the no. of test cases:";
 cin>>T;
 while(T--){
     cout<<"Enter the integer:";
     int N;
     cin>>N;
     prime(N);
 
 }

	return 0;
}
