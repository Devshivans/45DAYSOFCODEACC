#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int c=0,x,i;
	    cin>>x;
	    for(i=2;i<x;i++){
	        if(x%i==0){
	            break;
	        }
	    }
	    if(x==i){
	        cout<<"yes"<<endl;
	    }
	    else{
	        cout<<"no"<<endl;
	    }
	}
	return 0;
}
