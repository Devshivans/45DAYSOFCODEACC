#include <stdio.h>


int main() {
	int T;
	printf("Enter how many water you drink in a day");
	scanf("%d",&T);

	while(T--){
	    int X;
	    scanf("%d",&X);

	    if (X >= 2000){
	       
	    printf("yes,you take the doctor's advice");
		}else{
	        
	    printf("no,you didn't take the doctor's advice");
		}
	}
	return 0;
}
